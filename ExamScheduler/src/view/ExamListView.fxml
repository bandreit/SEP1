<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="516.0" prefWidth="1187.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ExamListViewController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="422.0" prefWidth="199.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addExamButtonPressed" prefHeight="53.0" prefWidth="154.0" text="Add exam">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" prefHeight="53.0" prefWidth="154.0" text="Remove exam" onAction="#removeExamButtonPressed">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" prefHeight="53.0" prefWidth="154.0" text="Edit exam">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <VBox prefHeight="60.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
<!--                        <Image url="VIA_Logo.png" />-->
                     </image>
                  </ImageView>
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox layoutX="213.0" layoutY="11.0" prefHeight="439.0" prefWidth="946.0">
         <children>
            <TableView fx:id="examListTable" prefHeight="426.0" prefWidth="802.0">
              <columns>
                <TableColumn fx:id="courseColumn" prefWidth="120.0" text="Course" />
                  <TableColumn fx:id="oralWrittenColumn" prefWidth="120.0" text="O/W" />
                  <TableColumn fx:id="dateColumn" prefWidth="120.0" text="Start Time" />
                  <TableColumn fx:id="date2Column" prefWidth="120.0" text="End Time" />
                  <TableColumn fx:id="examinersColumn" prefWidth="120.0" text="Examiners" />
                  <TableColumn fx:id="roomColumn" prefWidth="120.0" text="Room" />
                  <TableColumn fx:id="externalExaminerColumn" prefWidth="120.0" text="Eternal examiners" />
                  <TableColumn fx:id="studentsColumn" prefWidth="120.0" text="Students" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <Label fx:id="errorLabel" layoutX="614.0" layoutY="466.0" text="Error Label" textFill="RED" />
   </children>
</AnchorPane>
